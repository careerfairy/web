{
   "name": "careerfairy-functions",
   "private": true,
   "main": "dist/index.js",
   "scripts": {
      "lint": "eslint --ext .ts .",
      "build": "npm run clean && npm run copy-files && tsc",
      "dev": "npm run watch | npm run start:emulators",
      "start:emulators": "cross-env FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080 firebase emulators:start --only auth,firestore,functions",
      "watch": "npm run clean && npm run copy-files && tsc --watch",
      "copy-files": "cpx \"src/**/*.{json,html}\" dist/",
      "clean": "rimraf dist/",
      "logs": "firebase functions:log"
   },
   "fireDependencies": {
      "@careerfairy/shared-lib": "../shared-lib"
   },
   "dependencies": {
      "@careerfairy/shared-lib": "*",
      "agora-access-token": "^2.0.4",
      "algoliasearch": "^4.12.1",
      "axios": "^0.26.0",
      "dotenv": "^8.2.0",
      "firebase-admin": "^10.0.2",
      "firebase-functions": "^3.18.1",
      "ical-generator": "^2.2.0",
      "lodash": "^4.17.20",
      "lodash.clonedeep": "^4.5.0",
      "lodash.debounce": "^4.0.8",
      "luxon": "^1.22.0",
      "mailgun-js": "^0.22.0",
      "nanoid": "^3.3.1",
      "postmark": "^2.3.5"
   },
   "engines": {
      "node": "14"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.12.0",
      "@typescript-eslint/parser": "^5.12.0",
      "cors": "^2.8.5",
      "cpx": "^1.5.0",
      "eslint": "^8.9.0",
      "eslint-config-google": "^0.14.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.25.4",
      "firebase-functions-test": "^0.3.3",
      "rimraf": "^3.0.2",
      "typescript": "^4.5.4",
      "cross-env": "^7.0.3"
   },
   "fireConfig": {}
}
