{
   "name": "@careerfairy/functions",
   "private": true,
   "main": "dist/functions/src/index.js",
   "scripts": {
      "lint": "eslint --ext .ts .",
      "build": "npm run clean && npm run copy-files && tsc",
      "dev": "npm run watch | npm run start:emulators",
      "start:emulators": "cross-env JAVA_TOOL_OPTIONS=-Xmx15g FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199 FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080 firebase emulators:start --only auth,firestore,functions,storage --export-on-exit --import ../../emulatorData/fetched",
      "test": "jest",
      "watch": "npm run clean && npm run copy-files && tsc --watch",
      "copy-files": "cpx \"src/**/*.{json,html}\" dist/functions/src",
      "kill-emulators": "npx kill-port 8080 9099 4000 5001 9199 4000",
      "clean": "rimraf dist/",
      "logs": "firebase functions:log"
   },
   "fireDependencies": {
      "@careerfairy/shared-lib": "../shared-lib"
   },
   "dependencies": {
      "@careerfairy/shared-lib": "*",
      "@google-cloud/bigquery": "^6.0.2",
      "agora-access-token": "^2.0.4",
      "algoliasearch": "^4.12.1",
      "axios": "^0.27.2",
      "dotenv": "^8.2.0",
      "firebase-admin": "^10.3.0",
      "firebase-functions": "^3.22.0",
      "firebase": "^9.18.0",
      "form-data": "^4.0.0",
      "graphql": "^16.5.0",
      "graphql-request": "^4.3.0",
      "ical-generator": "^3.6.0",
      "lodash": "^4.17.20",
      "luxon": "^1.22.0",
      "mailgun.js": "^8.0.1",
      "module-alias": "^2.2.2",
      "nanoid": "^3.3.1",
      "postmark": "^2.3.5",
      "yup": "^0.32.11"
   },
   "engines": {
      "node": "16"
   },
   "devDependencies": {
      "@types/luxon": "^1.22.0",
      "@types/jest": "^29.0.2",
      "@types/module-alias": "^2.0.1",
      "@typescript-eslint/eslint-plugin": "^5.12.0",
      "@typescript-eslint/parser": "^5.12.0",
      "cpx": "^1.5.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.9.0",
      "eslint-config-google": "^0.14.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.25.4",
      "jest": "^29.0.3",
      "rimraf": "^3.0.2",
      "ts-jest": "^29.0.1",
      "typescript": "^4.8.3"
   }
}
