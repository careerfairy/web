{
   "name": "@careerfairy/shared-ui",
   "private": true,
   "version": "0.0.0",
   "main": "./dist/index.js",
   "module": "./dist/index.mjs",
   "types": "./dist/index.d.ts",
   "scripts": {
      "build": "tsup",
      "dev": "tsup --watch",
      "lint": "eslint --max-warnings 0 .",
      "lint-fix": "eslint --fix .",
      "tsc": "tsc -p tsconfig.json",
      "test": "jest --verbose",
      "test-watch": "jest --verbose --watch",
      "test-coverage": "jest --coverage --verbose",
      "storybook": "storybook dev -p 6202 --quiet",
      "storybook-build": "storybook build -o build-storybook-static",
      "update-dependencies": "npx npm-check-updates -u"
   },
   "dependencies": {
      "@careerfairy/shared-lib": "*",
      "@careerfairy/config-mui": "*"
   },
   "devDependencies": {
      "@careerfairy/config-tsconfig": "*"
   }
}
