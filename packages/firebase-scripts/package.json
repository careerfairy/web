{
   "name": "@careerfairy/firebase-scripts",
   "private": true,
   "version": "0.0.0",
   "main": "./dist/index.js",
   "scripts": {
      "build": "npm run clean && npm run copy-files && tsc",
      "dev": "npm run clean && npm run copy-files && tsc --watch",
      "copy-files": "cpx \"src/**/*.{json}\" dist/",
      "clean": "rimraf dist/",
      "script": "node dist/",
      "download-CSV-fieldAndLevelOfStudy": "node ./dist/scripts/queryAndSaveFieldAndLevelOfStudyToCsv.js NEXT_PUBLIC_FIREBASE_EMULATORS=false",
      "backfill-fieldOfStudy": "node ./dist/migrations/backfill-fieldOfStudy.js"
   },
   "dependencies": {
      "@careerfairy/shared-lib": "*",
      "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
      "@tensorflow/tfjs": "^3.18.0",
      "dotenv": "^16.0.1",
      "enquirer": "^2.3.6",
      "firebase": "^8.10.1",
      "firebase-admin": "^10.3.0",
      "objects-to-csv": "^1.3.6"
   },
   "devDependencies": {
      "cpx": "^1.5.0",
      "rimraf": "^3.0.2",
      "typescript": "^4.5.4"
   }
}
