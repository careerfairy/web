{
   "cli": {
      "version": ">= 12.6.0",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "env": {
            "EAS_BUILD_DISABLE_COCOAPODS_CACHE": "1"
         }
      },
      "preview": {
         "distribution": "internal",
         "env": {
            "EAS_BUILD_DISABLE_COCOAPODS_CACHE": "1"
         }
      },
      "production": {
         "autoIncrement": true,
         "env": {
            "EAS_BUILD_DISABLE_COCOAPODS_CACHE": "1"
         }
      }
   },
   "submit": {
      "production": {}
   }
}
