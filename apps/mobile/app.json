{
   "expo": {
      "name": "CareerFairy",
      "slug": "careerfairy-mobile-app",
      "version": "0.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/careerfairy.png",
      "scheme": "myapp",
      "userInterfaceStyle": "automatic",
      "splash": {
         "image": "./assets/images/careersplash.jpg",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "ios": {
         "supportsTablet": true,
         "infoPlist": {
            "NSCameraUsageDescription": "This app uses the camera to take photos and record videos.",
            "NSMicrophoneUsageDescription": "This app uses the microphone to record audio.",
            "NSUserNotificationUsageDescription": "This app uses notifications to inform you about important updates and messages."
         }
      },
      "android": {
         "permissions": [
            "CAMERA",
            "RECORD_AUDIO",
            "RECEIVE_BOOT_COMPLETED",
            "VIBRATE"
         ],
         "adaptiveIcon": {
            "foregroundImage": "./assets/images/careerfairy.png",
            "backgroundColor": "#ffffff"
         },
         "package": "com.admin_product_team.careerfairymobileapp"
      },
      "web": {
         "bundler": "metro",
         "output": "static",
         "favicon": "./assets/images/careerfairy.png"
      },
      "plugins": ["expo-router", "expo-secure-store"],
      "experiments": {
         "typedRoutes": true
      },
      "extra": {
         "eas": {
            "projectId": "52368a9d-be31-4ef9-be0a-3e9e1de4e098"
         }
      },
      "owner": "admin_product_team"
   }
}
