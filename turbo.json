{
   "$schema": "https://turborepo.org/schema.json",
   "globalEnv": [
      "NODE_ENV",
      "ENABLE_ROOT_PATH_BUILD_CACHE",
      "SENTRY_AUTH_TOKEN",
      "GRAPHCMS_PROJECT_API",
      "GRAPHCMS_PROD_AUTH_TOKEN",
      "GRAPHCMS_DEV_AUTH_TOKEN",
      "GRAPHCMS_PREVIEW_SECRET",
      "WEBFLOW_URL",
      "POSTMARK_TEMPLATE_TRIAL_END_SPARKS",
      "POSTMARK_TEMPLATE_MANUAL_EMAIL",
      "NEXT_PUBLIC_TALENT_GUIDE_HYGRAPH_URL",
      "HYGRAPH_TALENT_GUIDE_PROD_TOKEN",
      "HYGRAPH_TALENT_GUIDE_PREVIEW_TOKEN",
      "HYGRAPH_TALENT_GUIDE_PREVIEW_SECRET",
      "TEST_STRIPE_SECRET_KEY"
   ],
   "remoteCache": {
      "enabled": true
   },
   "tasks": {
      "build": {
         "dependsOn": ["^build"],
         "outputs": ["dist/**", ".next/**", "build/**", "!.next/cache/**"]
      },
      "test": {
         "dependsOn": ["build"],
         "outputs": ["coverage/**"]
      },
      "clean": {
         "dependsOn": ["^clean"]
      },
      "start": {
         "dependsOn": ["build"],
         "cache": false
      },
      "webapp:report": {},
      "test:e2e-webapp": {
         "dependsOn": ["build"],
         "outputs": ["test-results/**", "playwright-report/**"]
      },
      "test:e2e-webapp-debug": {
         "dependsOn": ["build"],
         "outputs": ["test-results/**", "playwright-report/**"]
      },
      "lint": {},
      "dev": {
         "cache": false
      },
      "native": {
         "cache": false
      }
   }
}
