{
   "name": "careerfairy-monorepo",
   "version": "0.0.0",
   "private": true,
   "workspaces": [
      "apps/*",
      "packages/*"
   ],
   "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --parallel",
      "test": "turbo run test",
      "clean": "turbo run clean",
      "start": "turbo run start --parallel",
      "storybook": "cd apps/web && npm run storybook",
      "test:e2e-webapp": "cross-env NODE_ENV=test turbo run test:e2e-webapp --filter=@careerfairy/webapp --filter=@careerfairy/functions",
      "test:e2e-webapp-debug": "cross-env NODE_ENV=test turbo run test:e2e-webapp-debug --filter=@careerfairy/webapp --filter=@careerfairy/functions",
      "webapp:report": "npm run playwright:report -w @careerfairy/webapp",
      "lint": "turbo run lint --parallel",
      "format": "prettier --write --ignore-unknown .",
      "prepare": "husky install",
      "build:packages": "turbo run build --ignore=\"apps/**/*\" --no-deps",
      "build:functions": "turbo run build --filter=@careerfairy/functions",
      "build:scripts": "turbo run build --filter=@careerfairy/firebase-scripts",
      "build:shared-lib": "turbo run build --filter=@careerfairy/shared-lib",
      "build:seed-data": "turbo run build --filter=@careerfairy/seed-data",
      "build:fetch-firestore-data": "turbo run build --filter=@careerfairy/fetch-firestore-data",
      "build:webapp": "turbo run build --filter=@careerfairy/webapp",
      "lint:functions": "turbo run lint --filter=@careerfairy/functions",
      "deploy:rules": "firebase deploy --only firestore:rules",
      "prepare:manual": "if [[ $NODE_ENVIRONMENT != \"production\" ]]; then husky install; fi",
      "start:webapp": "npx firebase emulators:exec \"npm run start -w @careerfairy/webapp\" --ui --only auth,firestore,functions"
   },
   "devDependencies": {
      "@rxdi/firelink": "^0.9.2",
      "cross-env": "^7.0.3",
      "firebase-tools": "^12.4.2",
      "husky": "^7.0.4",
      "lint-staged": "^12.4.1",
      "prettier": "^2.8.8",
      "turbo": "^1.10.16",
      "vercel": "^31.0.1"
   },
   "lint-staged": {
      "**/*": "prettier --write --ignore-unknown"
   },
   "packageManager": "npm@9.8.1",
   "engines": {
      "npm": ">=9"
   }
}
