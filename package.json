{
   "name": "careerfairy-monorepo",
   "version": "0.0.0",
   "private": true,
   "workspaces": [
      "apps/*",
      "packages/*"
   ],
   "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --parallel",
      "test": "turbo test",
      "clean": "turbo run clean",
      "start": "turbo run start --parallel",
      "storybook": "turbo storybook",
      "storybook-build": "turbo storybook-build",
      "test:e2e-webapp": "cross-env NODE_ENV=test turbo run test:e2e-webapp --filter=@careerfairy/webapp --filter=@careerfairy/functions",
      "test:e2e-webapp-debug": "cross-env NODE_ENV=test turbo run test:e2e-webapp-debug --filter=@careerfairy/webapp --filter=@careerfairy/functions",
      "webapp:report": "npm run playwright:report -w @careerfairy/webapp",
      "lint": "turbo run lint --parallel",
      "format": "prettier --write --ignore-unknown .",
      "prepare": "husky install",
      "build:packages": "turbo run build --ignore=\"apps/**/*\" --no-deps",
      "build:functions": "turbo run build --filter=@careerfairy/functions",
      "build:scripts": "turbo run build --filter=@careerfairy/firebase-scripts",
      "build:shared-lib": "turbo run build --filter=@careerfairy/shared-lib",
      "build:seed-data": "turbo run build --filter=@careerfairy/seed-data",
      "build:fetch-firestore-data": "turbo run build --filter=@careerfairy/fetch-firestore-data",
      "build:webapp": "turbo run build --filter=@careerfairy/webapp",
      "lint:functions": "turbo run lint --filter=@careerfairy/functions",
      "deploy:rules": "firebase deploy --only firestore:rules",
      "prepare:manual": "if [[ $NODE_ENVIRONMENT != \"production\" ]]; then husky install; fi",
      "start:webapp": "npx firebase emulators:exec \"npm run start -w @careerfairy/webapp\" --ui --only auth,firestore,functions"
   },
   "dependencies": {
      "@emotion/cache": "^11.7.1",
      "@emotion/react": "^11.11.1",
      "@emotion/server": "^11.4.0",
      "@emotion/styled": "^11.11.0",
      "@mui/icons-material": "^5.14.15",
      "@mui/material": "^5.14.16",
      "next": "^13.5.6",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
   },
   "devDependencies": {
      "@careerfairy/config-tsconfig": "*",
      "@rxdi/firelink": "^0.9.2",
      "@storybook/addon-a11y": "^7.5.2",
      "@storybook/addon-essentials": "^7.5.2",
      "@storybook/addon-interactions": "^7.5.2",
      "@storybook/addon-links": "^7.5.2",
      "@storybook/addon-storysource": "^7.5.2",
      "@storybook/blocks": "^7.5.2",
      "@storybook/nextjs": "^7.5.2",
      "@storybook/react": "^7.5.2",
      "@storybook/testing-library": "^0.2.2",
      "@testing-library/dom": "9.3.3",
      "@testing-library/jest-dom": "5.17.0",
      "@testing-library/react": "14.0.0",
      "@testing-library/user-event": "14.5.1",
      "@types/node": "20.8.3",
      "@types/react": "^18.2.32",
      "@types/react-dom": "^18.2.14",
      "@types/jest": "29.5.5",
      "cpx": "^1.5.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.43.0",
      "eslint-config-next": "^13.5.6",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-react": "^7.33.2",
      "eslint-plugin-storybook": "^0.6.15",
      "firebase-tools": "^12.4.2",
      "husky": "^7.0.4",
      "lint-staged": "^12.4.1",
      "prettier": "^2.8.8",
      "react-test-renderer": "^18.2.0",
      "rimraf": "^3.0.2",
      "storybook": "^7.5.2",
      "jest": "29.7.0",
      "jest-config": "29.7.0",
      "jest-environment-jsdom": "29.7.0",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "tsup": "^7.2.0",
      "turbo": "^1.10.16",
      "typescript": "^5.2.2",
      "vercel": "^31.0.1"
   },
   "lint-staged": {
      "**/*": "prettier --write --ignore-unknown"
   },
   "packageManager": "npm@9.8.1",
   "engines": {
      "npm": ">=9"
   }
}
